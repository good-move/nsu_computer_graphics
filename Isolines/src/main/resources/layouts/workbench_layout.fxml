<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.canvas.Canvas?>

<?import javafx.scene.image.ImageView?>
<AnchorPane
        xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml"
        fx:controller="ru.nsu.fit.g15201.boltava.presentation_layer.workbench.WorkbenchPresenter"
>
    <StackPane
            fx:id="wrapperPane"
            styleClass="wrapper-pane"
            AnchorPane.topAnchor="0.0"
            AnchorPane.bottomAnchor="0.0"
            AnchorPane.leftAnchor="0.0"
            AnchorPane.rightAnchor="0.0"
            minWidth="400"
            minHeight="400"
    >
        <Canvas fx:id="colorMapLayer" />
        <Canvas fx:id="gridLayer"/>
        <Canvas fx:id="isolinesLayer"/>
        <Canvas fx:id="intersectionsLayer"/>
    </StackPane>
</AnchorPane>
