<?import javafx.scene.control.ToolBar?>


<?import javafx.scene.layout.HBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tooltip?>
<ToolBar
        fx:controller="ru.nsu.fit.g15201.boltava.presentation_layer.toolbar.ToolbarView"
        xmlns:fx="http://javafx.com/fxml"
>
    <HBox>

        <Button onMouseClicked="#onOpenFile"
                styleClass="toolbar-btn, open-file-btn"
                text="Open"
        >
            <tooltip><Tooltip text="Open image"/></tooltip>
        </Button>

    </HBox>

</ToolBar>